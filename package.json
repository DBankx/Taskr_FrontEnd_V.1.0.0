  "scripts": {
    "compile:sass": "node-sass src/assets/sass/index.scss src/assets/css/style.comp.css ",
    "watch:sass": "node-sass src/assets/sass/index.scss src/assets/css/style.css --watch",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' src/assets/css/style.comp.css -o src/assets/css/style.prefix.css",
    "compress:css": "node-sass src/assets/css/style.prefix.css src/assets/css/style.css --output-style compressed",
    "build:css": "npm-run-all compile:sass prefix:css compress:css",
    "start": "react-scripts start -W",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
